{
  "name": "@caliorg/a2jdeps",
  "version": "7.0.7",
  "description": "Shared dependencies for A2J Author, A2J Viewer, and A2J Document Assembly Tool (DAT)",
  "main": "",
  "scripts": {
    "lint": "standard --fix --env mocha",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "AGPL-3.0-only",
  "steal": {
    "paths": {
      "tokenfield-jquery": "node_modules/bootstrap-tokenfield/node_modules/jquery/dist/jquery.js"
    },
    "map": {
      "chai": "chai/chai"
    },
    "plugins": [
      "steal-less",
      "steal-stache"
